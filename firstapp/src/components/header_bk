import React, {Component} from 'react';

class Header extends Component {
    render(){
        const styles = {
            header: {
                background:'teal'
            },
            logo:{
                color:'#fff',
                textAlign:'center',
                fontSize:'30px'
            }
        }
        return(
            <header style={styles.header}>
                <div style={styles.logo}>Logo</div>
                <center>
                    <input/>
                </center>
            </header>
        )
    }
}

export default Header;




